ww/html/React_Assignment2/my-react-app/src/pages/ContactUS.js",["77"],[],"/var/www/html/React_Assignment2/my-react-app/src/pages/CartPage.js",[],[],"/var/www/html/React_Assignment2/my-react-app/src/pages/ProductPage.js",["78"],[],"import React from 'react';\nimport {  useState , useEffect} from \"react\";\nimport axios from \"axios\";\nimport './Product.css';\nfunction ProductPage() {\n    const [product, setProduct] = useState([]);\n    const [cartItems, setCartItems] = useState([]);\n    useEffect(() => {\n        // Load cart items from local storage\n        const savedCartItems = JSON.parse(localStorage.getItem(\"cartItems\") || \"[]\");\n        setCartItems(savedCartItems);\n\n        fetch(\"/product.json\")\n            .then((response) => response.json())\n            .then((data) => setProduct(data))\n            .catch((error) => console.error(error));\n    }, []);\n\n    const handleAddToCart = (product) => {\n        // Check if the product is already in the cart\n        const existingItem = cartItems.find((item) => item.id === product.id);\n\n        if (existingItem) {\n            // Increment the quantity if the product is already in the cart\n            const updatedCartItems = cartItems.map((item) =>\n                item.id === product.id ? { ...item, quantity: item.quantity + 1 } : item\n            );\n            setCartItems(updatedCartItems);\n        } else {\n            // Add the product to the cart with a quantity of 1\n            const newCartItem = { id: product.id, name: product.name, price: product.price, quantity: 1 };\n            setCartItems([...cartItems, newCartItem]);\n        }\n    };\n\n    // Save cart items to local storage whenever the cart items state changes\n    useEffect(() => {\n        localStorage.setItem(\"cartItems\", JSON.stringify(cartItems));\n    }, [cartItems]);\n\n\n\n    return (\n\n\n        <div className=\"product-container\">\n            <ul className=\"product-card\">\n                {product.map((product, index) => (\n                    <li className=\"product\" key={index}>\n                        <div className=\"product-img\">\n                            <img src={product.image} alt={product.name} />\n                        </div>\n                        <div className=\"product-info\">\n                            <h3>{product.name}</h3>\n                            <p>{product.description}</p>\n                            <p>{product.price} Rs</p>\n                            <button onClick={ () => {if(window.confirm(\"Are you sure to add product in cart \"))\n                            { handleAddToCart(product)};}} className=\"cart\">Add to Cart</button>\n                        </div>\n                    </li>\n                ))}\n            </ul>\n        </div>\n    );\n}\n\nexport default ProductPage;\n",[],"/var/www/html/React_Assignment2/my-react-app/src/pages/Layout.js",["79","80","81","82"],[],"/var/www/html/React_Assignment2/my-react-app/src/pages/Payment.js",[],[],{"ruleId":"83","severity":1,"message":"84","line":1,"column":8,"nodeType":"85","messageId":"86","endLine":1,"endColumn":12},{"ruleId":"87","severity":1,"message":"88","line":7,"column":13,"nodeType":"89","endLine":7,"endColumn":76},{"ruleId":"87","severity":1,"message":"88","line":28,"column":50,"nodeType":"89","endLine":28,"endColumn":97},{"ruleId":"87","severity":1,"message":"88","line":32,"column":49,"nodeType":"89","endLine":32,"endColumn":95},{"ruleId":"87","severity":1,"message":"88","line":9,"column":17,"nodeType":"89","endLine":9,"endColumn":80},{"ruleId":"87","severity":1,"message":"88","line":14,"column":17,"nodeType":"89","endLine":14,"endColumn":88},{"ruleId":"87","severity":1,"message":"88","line":17,"column":17,"nodeType":"89","endLine":17,"endColumn":60},{"ruleId":"87","severity":1,"message":"88","line":18,"column":17,"nodeType":"89","endLine":18,"endColumn":58},{"ruleId":"87","severity":1,"message":"88","line":19,"column":17,"nodeType":"89","endLine":